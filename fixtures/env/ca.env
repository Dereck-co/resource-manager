# CA config

FABRIC_CA_HOME=/etc/hyperledger/fabric-ca-server
FABRIC_CA_SERVER_CA_NAME=ca.org1.hf.chainhero.io
FABRIC_CA_SERVER_CA_CERTFILE=/etc/hyperledger/fabric-ca-server-config/ca.org1.hf.chainhero.io-cert.pem
FABRIC_CA_SERVER_CA_KEYFILE=/etc/hyperledger/fabric-ca-server-config/81b673cf987e58182c6096616aff9d767012c54c2c609c4860999ff4fe71b702_sk
FABRIC_CA_SERVER_CFG_AFFILIATIONS_ALLOWREMOVE=true
FABRIC_CA_SERVER_CFG_IDENTITIES_ALLOWREMOVE=true

## CA TLS
FABRIC_CA_SERVER_TLS_ENABLED=true
FABRIC_CA_SERVER_TLS_CERTFILE=/etc/hyperledger/tls/peer/server.crt
FABRIC_CA_SERVER_TLS_KEYFILE=/etc/hyperledger/tls/peer/server.key
